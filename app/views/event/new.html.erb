<%= form_for :event, html: {class: "form"} do | form | %>
    <%= form.text_field :title, data: {test_id: 'title_field'} %>
    <%= form.text_field :description, data: {test_id: 'description_field'} %>

    <%= form.button "Create", data: {test_id: "week_event"} %>
    <%= form.button "Create", data: {test_id: "month_event"} %>
<% end %>
